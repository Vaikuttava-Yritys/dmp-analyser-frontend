{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.css", "/*.js", "/*.json", "/*.svg", "/*.png", "/*.jpg"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    {
      "route": "/about",
      "serve": "/about.html",
      "statusCode": 200
    },
    {
      "route": "/privacy",
      "serve": "/privacy.html",
      "statusCode": 200
    },
    {
      "route": "/feedback",
      "serve": "/feedback.html",
      "statusCode": 200
    },
    {
      "route": "/export",
      "serve": "/export.html",
      "statusCode": 200
    }
  ]
}
